
@{
    ViewBag.Title = "Index";
}


<style>
 
    h2 {
        margin: 20px 0px 20px 0px;
    }
    span[class="glyphicon glyphicon-cog"], 
    span[class="glyphicon glyphicon-backward"] 
                { 
        margin: 5px 5px 5px 5px; 

    }
</style>


<div class="container">
    <h2>Projekti</h2>

    <div class="row no-gutters">
        <div class="col-10 col-sm-4 col-md-4">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="~/Prijava/Index">Index</a></li>
                    <li class="breadcrumb-item"><a href="~/Izbornik/Izbornik">Izbornik</a></li>
                    <li class="breadcrumb-item active" aria-current="page">Projekt</li>
                </ol>
            </nav>
        </div>
    </div>



    <h3>Izaberi iz izbornika:</h3>
    <hr />
    <p>
        <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
        @Html.ActionLink("Upis projekta", "UpisProjekta", "Projekt")
    </p>
    <p><span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
        @Html.ActionLink("Popis projekata", "PopisProjekata", "Projekt")
    </p>
    <p><span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
        @Html.ActionLink("Izmjena projekata", "IzmjenaProjekata", "Projekt")
    </p>
    <p><span class="glyphicon glyphicon-backward" aria-hidden="true"></span>
        @Html.ActionLink("Povratak", "Izbornik", "Izbornik")
